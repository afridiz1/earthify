{
  "manifest_version": 2,
  "name": "Earthify",
  "version": "0.2",

  "permissions": [
    "activeTab", 
    "declarativeContent", 
    "storage",
    "tabs"
  ], 


  "pageAction": {
    "default_popup": "popup.html"
  },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "chrome_url_overrides" : {
    "newtab": "mainPage.html"
  },

  "background": {
    "scripts": ["background.js", "jquery-3.4.1.js"],
    "persistent": false
  }

}
